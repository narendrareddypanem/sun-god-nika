def is_prime(num):
    if num <= 1:
        return False
    for i in range(2, int(num**0.5) + 1):
        if num % i == 0:
            return False
    return True

def find_primes(limit):
    primes = []
    for num in range(2, limit + 1):
        if is_prime(num):
            primes.append(num)
    return primes

# Example usage:
limit = int(input("Enter the limit to find prime numbers: "))
primes = find_primes(limit)
print(f"Prime numbers up to {limit} are: {primes}")
